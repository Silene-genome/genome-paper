package mcmc;

import phylogeny.Path;

/**
 * This class is a container. It is used for returning with a Path and its proposal probability.
 * @author miklosi
 *
 */
public class PathMetropolisContainer {
	
	public double proposalProbability;
	public Path path;
	
	public String print(){
		return "Proposal probability: "+proposalProbability+"\npath: \n"+path.print();
	}

}
